        <result column="post_upd_user" property="postUpdUser" />
        <result column="post_upd_date" property="postUpdDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        post_id, post_name, post_type, post_ten_id, post_use_state, post_int_user, post_int_date, post_upd_user, post_upd_date
